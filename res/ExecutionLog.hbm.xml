<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.uw.css553.backend.entities.ExecutionLog" table="executionlog">
        <id name="id" type="string" column="LogId"/>
        <property name="workflowId" type="string" column="WorkflowId"/>
        <property name="inputURL" type="string" column="InputURL"/>
        <property name="executionStartTime" type="timestamp" column="ExecutionStartTime"/>
        <property name="executionEndTime" type="timestamp" column="ExecutionCompleteTime"/>
        <property name="returnValue" type="string" column="ReturnValue"/>
        <property name="returnMessage" type="string" column="ReturnMessage"/>
        <property name="executionNotes" type="string" column="ExecutionNotes"/>
        <property name="stepCount" type="integer" column="StepCount"/>
        <property name="createdBy" type="string" column="CreatedBy"/>
        <property name="creationTime" type="timestamp" column="CreationTime"/>
        <property name="modifiedBy" type="string" column="ModifiedBy"/>
        <property name="modificationTime" type="timestamp" column="ModificationTime"/>
        <property name="version" type="integer" column="Version"/>
    </class>
</hibernate-mapping>