<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.uw.css553.backend.entities.Action" table="workflowstep">
        <id name="id" type="string" column="StepId"/>
        <property name="name" type="string" column="StepName" />
        <property name="description" type="string" column="StepDescription"/>
        <property name="sequence" type="integer" column="StepSequence"/>
        <property name="outputURL" type="string" column="OutputURL"/>
        <property name="toolURL" type="string" column="ToolURL"/>
        <property name="createdBy" type="string" column="CreatedBy"/>
        <property name="creationTime" type="timestamp" column="CreationTime"/>
        <property name="modifiedBy" type="string" column="ModifiedBy"/>
        <property name="modificationTime" type="timestamp" column="ModificationTime"/>
        <property name="version" type="integer" column="Version"/>
    </class>
</hibernate-mapping>